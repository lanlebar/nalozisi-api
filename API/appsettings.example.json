{
  "AppSettings": {
    "Issuer": "https://your-issuer-url/",
    "Audience": "http://your-audience-url/",
    "Key": "your-secret-key"
  },
  "Storage": {
    "BasePath": "your-base-path"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FileSystem": {
    "ProfilePicsSizeLimit": 5242880,
    "ProfilePics": "your-profile-pics-path",
    "SupportedImageFormats": [ ".jpg", ".jpeg", ".png", ".webp", ".bmp", ".gif", ".tiff" ]
  },
  "NodeScripts": {
    "NodePath": "your-node-path",
    "ScriptsPath": "your-scripts-path"
  },
  "ApiKeys": {
    "Tmdb": "your-tmdb-key"
  },
  "ConnectionStrings": {
    "DefaultConnection": "your-connection-string"
  },
  "InternalApiSettings": {
    "BaseScrapeSearchLimit": "20"
  },
  "AllowedHosts": "*"
}
